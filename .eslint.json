{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "plugins": [
      "node"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "node/no-missing-require": "error"
    }
}
  